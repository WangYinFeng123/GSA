#ifndef Process_hpp
#define Process_hpp

#include <random>

class Process{
	
	public:
		Process{}();
		virtual ~Process(){};
		virtual double rate (double t,std::mt19937 & rng) = 0;
	
		virtual double P(double t, double T) = 0;	

}


class CIRProcess: public Process{

	public: 
		CIRProcess();
		virtual double rate (double t,std::mt19937 & rng);

		virtual double bondPrice(double t, double T);

	private:
		double A(double t, double T) const;
		double B(double t, double T) const;
}

#endif
